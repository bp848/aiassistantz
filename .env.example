# Copy this file to `.env.local` for local development (do not commit `.env.local`).
#
# Supabase
VITE_SUPABASE_URL="https://YOUR_PROJECT.supabase.co"
VITE_SUPABASE_ANON_KEY="YOUR_SUPABASE_ANON_KEY"
VITE_GOOGLE_SCOPES="https://www.googleapis.com/auth/userinfo.email,https://www.googleapis.com/auth/userinfo.profile,https://www.googleapis.com/auth/gmail.modify,https://www.googleapis.com/auth/calendar"
VITE_GOOGLE_CLIENT_ID="YOUR_GOOGLE_CLIENT_ID"
VITE_GOOGLE_CLIENT_SECRET="YOUR_GOOGLE_CLIENT_SECRET"

# Gemini (local dev)
# Production should set GEMINI_API_KEY in Vercel (server-side), not VITE_GEMINI_API_KEY.
VITE_GEMINI_API_KEY=""

# Optional: override proxy URL (e.g. staging)
VITE_GEMINI_PROXY_URL=""

# Demo mode
VITE_DEMO_MODE="false"

